@prefix datacite: <http://purl.org/spar/datacite/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lvont: <http://lexvo.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skg: <https://w3id.org/skg-if/ontology/> .
@prefix skg_sh_current: <https://w3id.org/skg-if/shapes/current/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix srv: <https://w3id.org/skg-if/extension/srv/ontology/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

skg_sh_current:APIProfileShape a sh:NodeShape ;
    sh:property [ sh:datatype xsd:anyURI ;
            sh:minCount 1 ;
            sh:path schema:url ],
        [ sh:datatype xsd:anyURI ;
            sh:minCount 1 ;
            sh:path dcat:endpointURL ] .

skg_sh_current:FacilityPortalShape a sh:NodeShape ;
    sh:property [ sh:class foaf:Organization ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path schema:provider ],
        [ sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path skos:prefLabel ],
        [ sh:minCount 0 ;
            sh:nodeKind sh:Literal ;
            sh:path skos:altLabel ] .

skg_sh_current:ServiceShape a sh:NodeShape ;
    sh:property [ sh:class skg:research_product ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:relatedResearchProduct ],
        [ sh:class foaf:Organization ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path dcterms:relation ],
        [ sh:class skos:Concept ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:invocationType ],
        [ sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:nodeKind sh:Literal ;
            sh:path dcterms:description ],
        [ sh:class srv:APIProfile ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path dcterms:conformsTo ],
        [ sh:class fabio:SubjectTerm ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:hasTopic ],
        [ sh:class skg:research_product ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:deploymentOf ],
        [ sh:class owl:Thing ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:geographicalAvailability ],
        [ sh:class schema:SoftwareSourceCode ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:deploymentOf ],
        [ sh:minCount 0 ;
            sh:nodeKind sh:Literal ;
            sh:path skos:altLabel ],
        [ sh:class srv:FacilityPortal ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path schema:provider ],
        [ sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path skos:prefLabel ],
        [ sh:class lvont:Language ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:supportedLanguage ],
        [ sh:class skos:Concept ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:eoscLifeCycleStatus ],
        [ sh:class foaf:Document ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path foaf:homepage ],
        [ sh:datatype xsd:string ;
            sh:minCount 0 ;
            sh:path srv:audienceJurisdiction ],
        [ sh:class datacite:Identifier ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path datacite:hasIdentifier ],
        [ sh:class skos:Concept ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:intendedAudienceRole ],
        [ sh:class fabio:Software ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path srv:deploymentOf ],
        [ sh:class owl:Thing ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path dcterms:subject ] ;
    sh:targetClass srv:Service .

skg_sh_current:SoftwareSourceCodeShape a sh:NodeShape ;
    sh:property [ sh:class foaf:Document ;
            sh:minCount 0 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path foaf:homepage ],
        [ sh:class schema:url ;
            sh:minCount 1 ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path schema:codeRepository ] .

skg_sh_current:StandardShape a sh:NodeShape ;
    sh:property [ sh:minCount 0 ;
            sh:nodeKind sh:Literal ;
            sh:path skos:prefLabel ] .

